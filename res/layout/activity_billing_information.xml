<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_billing_information"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.lastmanstandinglayout.BillingInformationActivity">

    <TextView
        android:text="@string/app_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView120"
        android:textSize="40sp"
        android:textColor="#45B"
        android:textStyle="normal|bold|italic"
        android:textAlignment="center"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="16dp"
        app:layout_constraintVertical_bias="0.050000012" />

    <TextView
        android:text="@string/profile_page_link"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView121"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="104dp"
        app:layout_constraintTop_toTopOf="parent"
        android:textColor="#45B"
        android:onClick="profile"/>

    <TextView
        android:text="@string/billing_information"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView122"
        android:textColor="#000"
        android:textStyle="normal|bold"
        android:textSize="24sp"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="16dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="16dp"
        android:layout_marginTop="128dp"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:text="@string/balance"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView123"
        android:textSize="18sp"
        android:textColor="#000"
        android:layout_marginEnd="216dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="216dp"
        android:layout_marginTop="32dp"
        app:layout_constraintTop_toBottomOf="@+id/textView122" />

    <TextView
        android:text="@string/balance_sample"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView124"
        android:textSize="18sp"
        android:layout_marginStart="24dp"
        app:layout_constraintLeft_toRightOf="@+id/textView123"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="32dp"
        app:layout_constraintTop_toBottomOf="@+id/textView122" />

    <TextView
        android:text="@string/add_funds"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView125"
        android:textSize="18sp"
        android:textColor="#45B"
        android:layout_marginTop="32dp"
        app:layout_constraintTop_toBottomOf="@+id/textView123"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="16dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="16dp" />

    <TextView
        android:text="@string/cash_out"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView126"
        android:textSize="18sp"
        android:textColor="#45B"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="16dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="16dp"
        android:layout_marginTop="32dp"
        app:layout_constraintTop_toBottomOf="@+id/textView125" />
</android.support.constraint.ConstraintLayout>
