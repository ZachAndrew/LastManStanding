<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_manage_league"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.lastmanstandinglayout2.ManageLeagueActivity">

    <TextView
        android:text="@string/player_name_sample"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView86"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/textView84"
        android:layout_marginStart="88dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="88dp"
        android:textSize="18sp" />

    <TextView
        android:text="@string/league_name_sample"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView67"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="40dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginRight="16dp"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="16dp"
        android:textSize="30sp"
        android:textColor="@color/blackText"
        android:textStyle="normal|bold" />

    <TextView
        android:text="@string/form_league_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView68"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="16dp"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="16dp"
        app:layout_constraintHorizontal_bias="0.18"
        android:textSize="18sp"
        android:textColor="@color/blackText"
        app:layout_constraintBaseline_toBaselineOf="@+id/editText11" />

    <EditText
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:ems="10"
        android:id="@+id/editText11"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="16dp"
        app:layout_constraintLeft_toRightOf="@+id/textView68"
        android:layout_marginStart="24dp"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="40dp"
        app:layout_constraintTop_toBottomOf="@+id/textView67"
        app:layout_constraintHorizontal_bias="0.66"
        android:hint="@string/league_name_sample" />

    <TextView
        android:text="@string/form_league_code"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView82"
        app:layout_constraintRight_toRightOf="@+id/textView68"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="16dp"
        app:layout_constraintHorizontal_bias="1.0"
        android:textSize="18sp"
        android:textColor="@color/blackText"
        app:layout_constraintTop_toTopOf="@+id/textView83" />

    <TextView
        android:text="@string/league_code_sample"
        android:layout_width="184dp"
        android:layout_height="wrap_content"
        android:id="@+id/textView83"
        android:textSize="18sp"
        android:layout_marginStart="24dp"
        app:layout_constraintLeft_toRightOf="@+id/textView82"
        android:layout_marginLeft="24dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="16dp"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/editText11"
        app:layout_constraintHorizontal_bias="0.41" />

    <TextView
        android:text="@string/remove_players"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView84"
        android:textSize="20sp"
        android:textColor="@color/blackText"
        android:textStyle="normal|bold"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="16dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="16dp"
        android:layout_marginTop="48dp"
        app:layout_constraintTop_toBottomOf="@+id/textView83" />

    <TextView
        android:text="@string/player_name_sample"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView85"
        android:textSize="18sp"
        app:layout_constraintLeft_toLeftOf="@+id/textView86"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/textView86" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@android:drawable/ic_delete"
        android:id="@+id/imageView"
        android:contentDescription="@string/remove_button"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toRightOf="@+id/textView86"
        android:layout_marginLeft="16dp"
        app:layout_constraintTop_toTopOf="@+id/textView86"
        app:layout_constraintBottom_toBottomOf="@+id/textView86"
        app:layout_constraintVertical_bias="0.45" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@android:drawable/ic_delete"
        android:id="@+id/imageView2"
        app:layout_constraintTop_toTopOf="@+id/textView85"
        app:layout_constraintBottom_toBottomOf="@+id/textView85"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toRightOf="@+id/textView85"
        android:layout_marginLeft="16dp"
        app:layout_constraintVertical_bias="0.45"
        android:contentDescription="@string/remove_button" />

    <TextView
        android:text="@string/requests"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView87"
        android:textSize="20sp"
        android:textStyle="normal|bold"
        android:textColor="@color/blackText"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="16dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="16dp"
        android:layout_marginTop="48dp"
        app:layout_constraintTop_toBottomOf="@+id/textView85" />

    <TextView
        android:text="@string/player_name_sample"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView88"
        android:layout_marginStart="88dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="88dp"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/textView87"
        android:textSize="18sp" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@android:drawable/ic_input_add"
        android:id="@+id/imageView3"
        app:layout_constraintTop_toTopOf="@+id/textView88"
        app:layout_constraintBottom_toBottomOf="@+id/textView88"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toRightOf="@+id/textView88"
        android:layout_marginLeft="16dp"
        app:layout_constraintVertical_bias="0.45"
        android:contentDescription="@string/accept_button" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@android:drawable/ic_delete"
        android:id="@+id/imageView4"
        app:layout_constraintTop_toTopOf="@+id/imageView3"
        app:layout_constraintBottom_toBottomOf="@+id/imageView3"
        android:layout_marginStart="8dp"
        app:layout_constraintLeft_toRightOf="@+id/imageView3"
        android:layout_marginLeft="8dp"
        app:layout_constraintVertical_bias="0.0"
        android:contentDescription="@string/remove_button" />

    <Button
        android:text="@string/delete_league"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button8"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="16dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="16dp"
        android:layout_marginTop="48dp"
        app:layout_constraintTop_toBottomOf="@+id/textView88" />
</android.support.constraint.ConstraintLayout>
